<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="student" class="DependencyInjection.SetterGetter.Student">
        <property name="studentName" value="Jahedul" />
    </bean>
    <bean id = "student1" class="DependencyInjection.Constructor.Student">
        <constructor-arg name="studentName" value="Jahedul" />
        <constructor-arg name ="Id" value="123456"/>
    </bean>

</beans>